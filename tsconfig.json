{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"noEmit": false,
		"target": "es6",
		"module": "es6",
		"esModuleInterop": true,
		"outDir": "dist",
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"strict": false,
		"typeRoots": ["./types", "./node_modules/@types"]
	}
}
